{
  "file": "/data/paper.yml",
  "ops": [
    {
      "$set": {
        "path": "$.verbose",
        "value": true
      }
    },
    {
      "$set": {
        "path": "$.settings['velocity-support'].enabled",
        "value": true
      }
    },
    {
      "$set": {
        "path": "$.settings['velocity-support'].online-mode",
        "value": false
      }
    },
    {
      "$set": {
        "path": "$.settings['velocity-support'].secret",
        "value": "secret!"
      }
    }
  ]
}
