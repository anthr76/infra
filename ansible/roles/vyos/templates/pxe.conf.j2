allow booting;
next-server {{ static_dns["netboot.nwk1.rabbito.tech"] }};

if option arch = 00:07 {
    filename "netboot.xyz.efi";
} elsif option arch = 00:00 {
    filename "netboot.xyz.kpxe";
} elsif option arch = 00:0b {
    filename "netboot.xyz-arm64.efi";
} else {
    filename "netboot.xyz.efi";
}
